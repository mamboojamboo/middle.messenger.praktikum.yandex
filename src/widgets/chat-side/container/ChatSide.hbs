<section class="{{ styles.side }}">
	<h1 class="{{ styles.side__title }}">{{ title }}</h1>

	<div class="{{ styles.side__usercard }}">
		{{{ User }}}
		{{{ EditUserProfile }}}
	</div>

	{{{ SearchDialog }}}

	<div class="{{ styles.side__chatlist }}">
		{{#each chatList}}
			<div data-dialog="DIALOG">
				{{{ this }}}
			</div>
		{{/each}}
	</div>
</section>
